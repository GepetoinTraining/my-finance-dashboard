{
  "builds": [
    {
      "src": "src/api/**/*.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}